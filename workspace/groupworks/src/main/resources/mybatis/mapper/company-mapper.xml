<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="CompanyMapper">

    <!--    회사추가    -->
    <insert id="insertCompany">
        INSERT INTO COMPANY
        (
        NO
        , NAME
        )
        VALUES
        (
        SEQ_COMPANY_NO.NEXTVAL
        , #{name}
        )
    </insert>


</mapper>